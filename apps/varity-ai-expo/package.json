{
	"name": "varity-ai-expo",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"scripts": {
		"start": "expo start --dev-client",
		"ios": "expo run:ios",
		"android": "expo run:android",
		"build:dev": "eas build --profile development",
		"build:dev-sim": "eas build --profile development-simulator",
		"build:preview": "eas build --profile preview",
		"build:prod": "eas build --profile production",
		"prebuild": "expo prebuild",
		"web": "expo start --web",
		"build:web": "expo export -p web",
		"deploy:preview": "eas deploy --alias preview",
		"deploy:prod": "eas deploy --prod",
		"update": "bun update --latest",
		"expo-doctor": "bunx expo-doctor",
		"expo-doctor:fix": "bunx expo install --check"
	},
	"dependencies": {
		"@auth/core": "^0.39.1",
		"@convex-dev/auth": "^0.0.84",
		"@convex-dev/react-query": "^0.0.0-alpha.8",
		"@expo/vector-icons": "^14.1.0",
		"@react-navigation/bottom-tabs": "^7.3.10",
		"@react-navigation/elements": "^2.4.2",
		"@react-navigation/native": "^7.1.6",
		"@tamagui/config": "1.126.12",
		"@tamagui/core": "1.126.12",
		"@tamagui/font-inter": "1.126.12",
		"@tamagui/lucide-icons": "1.126.12",
		"@tamagui/react-native-media-driver": "1.126.12",
		"@tamagui/shorthands": "1.126.12",
		"@tamagui/themes": "1.126.12",
		"@tamagui/toast": "1.126.12",
		"@tamagui/web": "1.126.12",
		"convex": "^1.24.1",
		"expo": "~53.0.9",
		"expo-auth-session": "^6.1.5",
		"expo-blur": "~14.1.4",
		"expo-constants": "~17.1.6",
		"expo-dev-client": "~5.1.8",
		"expo-font": "~13.3.1",
		"expo-haptics": "~14.1.4",
		"expo-image": "~2.1.7",
		"expo-linking": "~7.1.5",
		"expo-navigation-bar": "^4.2.4",
		"expo-router": "~5.0.6",
		"expo-secure-store": "~14.2.3",
		"expo-splash-screen": "~0.30.8",
		"expo-status-bar": "~2.2.3",
		"expo-symbols": "~0.4.4",
		"expo-system-ui": "~5.0.7",
		"expo-updates": "~0.28.12",
		"expo-web-browser": "~14.1.6",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-native": "0.79.2",
		"react-native-gesture-handler": "~2.24.0",
		"react-native-reanimated": "~3.17.5",
		"react-native-safe-area-context": "5.4.0",
		"react-native-screens": "~4.10.0",
		"react-native-web": "~0.20.0",
		"react-native-webview": "13.13.5",
		"tamagui": "^1.126.12"
	},
	"devDependencies": {
		"@babel/core": "^7.27.1",
		"@tamagui/babel-plugin": "^1.126.12",
		"@types/react": "~19.0.10",
		"eslint": "^9.26.0",
		"eslint-config-expo": "~9.2.0",
		"typescript": "~5.8.3"
	},
	"expo": {
		"doctor": {
			"reactNativeDirectoryCheck": {
				"exclude": [
					"@tamagui/core",
					"@tamagui/font-inter",
					"@tamagui/lucide-icons",
					"@tamagui/react-native-media-driver",
					"@tamagui/shorthands",
					"@tamagui/themes",
					"@tamagui/toast",
					"tamagui",
					"@tamagui/config",
					"@tamagui/web"
				]
			}
		},
		"install": {
			"exclude": []
		}
	},
	"private": true
}
