		"eslint-config-expo": "~9.2.0",
		"typescript": "~5.8.3"
	},
	"expo": {
		"doctor": {
			"reactNativeDirectoryCheck": {
				"exclude": [
					"@tamagui/core",
					"@tamagui/font-inter",
					"@tamagui/lucide-icons",
					"@tamagui/react-native-media-driver",
					"@tamagui/shorthands",
					"@tamagui/themes",
					"@tamagui/toast",
					"tamagui",
					"@tamagui/config",
					"@tamagui/web"
